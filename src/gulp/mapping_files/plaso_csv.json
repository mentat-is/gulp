{
    "metadata": {
        "plugin": [
            "csv.py"
        ]
    },
    "mappings": {
        "default": {
            "description": "plaso psort csv format (https://plaso.readthedocs.io/en/latest/sources/user/Output-and-formatting.html#dynamic-runtime-fields)",
            "default_source": "plaso",
            "default_context": "plaso",
            "fields": {
                "datetime":{
                    "ecs": [
                        "@timestamp"
                    ]
                },
                "description": {
                    "ecs": [
                        "gulp.event_description"
                    ]
                },
                "display_name": {
                    "ecs": [
                        "file.path"
                    ]
                },
                "filename": {
                    "ecs": [
                        "file.name"
                    ]
                },
                "host": {
                    "ecs": [
                        "host.name"
                    ]
                },
                "hostname": {
                    "ecs": [
                        "host.hostname"
                    ]
                },
                "inode": {
                    "ecs": [
                        "file.inode"
                    ]
                },
                "macb": {
                    "ecs": [
                        "gulp.file.macb"
                    ]
                },
                "message": {
                    "ecs": [
                        "gulp.event_message"
                    ]
                },
                "sourcetype": {
                    "ecs": [
                        "event.provider"
                    ]
                },
                "timezone": {
                    "ecs": [
                        "event.timezone"
                    ]
                },
                "source": {
                    "ecs": [
                        "event.code"
                    ]
                },
                "user": {
                    "ecs": [
                        "user.name"
                    ]
                },
                "username": {
                    "ecs": [
                        "user.name"
                    ]
                },
                "zone": {
                    "ecs": [
                        "event.timezone"
                    ]
                }
            }            
        }
    }
}