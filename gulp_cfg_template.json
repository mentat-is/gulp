{
  // bind_to is the address:port to bind to (default=0.0.0.0:8080), may be overridden by GULP_BIND_TO_ADDR, GULP_BIND_TO_PORT environment variables
  "bind_to": null,
  // each worker process will run this number of tasks concurrently (default=16)
  // when using adaptive concurrency, this is the base for calculating max tasks per worker process
  "concurrency_num_tasks": 16,
  // if set, `concurrency_num_tasks` is enabled and max tasks per worker process is determined by the opensearch/postgresql cluster configurations.
  "concurrency_adaptive_num_tasks": true,
  // used to determine concurrency_num_tasks when `concurrency_adaptive_num_tasks` is set (default=1, single opensearch node)
  "concurrency_opensearch_num_nodes": 1,
  // used to determine concurrency_num_tasks when `concurrency_adaptive_num_tasks` is set (default=1, single postgresql node)
  "concurrency_postgres_num_nodes": 1,
  // used when `concurrency_adaptive_num_tasks` is set, caps the max tasks per worker process to this value (default=64)
  "concurrency_tasks_cap_per_process": 64,
  // only for debugging, abort ingestion of the current file on first error when indexing a document (default=true)
  "debug_abort_on_opensearch_ingestion_error": true,
  // allow any token (even non existent) to be used as admin, also ignores token expiration time (default=false)
  "debug_allow_any_token_as_admin": false,
  // enable insecure passwords for user during development
  "debug_allow_insecure_passwords": false,
  // enable debug for collaboration engine (prints sql queries and more, default=false)
  "debug_collab": false,
  // enable dry run mode (= do not update data on storage) for enrich plugins (default: false)
  "debug_enrich_dry_run": false,
  // if set, will ignore missing websocket connections (default=false)
  "debug_ignore_missing_ws": false,
  // prevent token expiration for debugging purposes, ignored if debug_allow_any_token_as_admin is set to true (default=false)
  "debug_no_token_expiration": false,
  // enforce https
  "https_enforce": false,
  // enforce client certificates signed by gulp-ca for https
  "https_enforce_client_certs": false,
  // default for index template creation
  "index_template_default_total_fields_limit": 10000,
  // if set, tweaks the "ignore_above" setting for dynamically generated keyword fields in the index (default=not specified, use default)
  "index_dynamic_keyword_ignore_above": 1500,
  // default for index template creation
  "index_template_default_refresh_interval": "5s",
  // defaults to true, allows fields not defined in the mapping to be ingested (generates gulp.unmapped.* keys in the document).
  // warning: this may affect sigma rules matching if set to false, but may be useful to enforce strict mappings in some cases (i.e. eases AI workflow).
  "ingestion_allow_unmapped_fields": true,
  // size of the documents chunk when ingesting/querying (default=1000). if you're getting websocket disconnections (PayloadTooBig), try lowering this value or use GulpPluginParameters.override_chunk_size
  // on file ingestion only, this is ignored if "ingestion_documents_adaptive_chunk_size" is set.
  "ingestion_documents_chunk_size": 1000,
  // if set, ingestion chunk depends on the source file size (applies to file ingestion only)
  "ingestion_documents_adaptive_chunk_size": true,
  // failed events in a file to consider the file ABORTED (0=disabled, ignored for raw ingestions)
  "ingestion_evt_failure_threshold": 5000,
  // how many times to retry ingestion on error (default=3)
  "ingestion_retry_max": 3,
  // how many seconds between ingestion retries on error (default=1)
  "ingestion_retry_delay": 1,
  // after how many chunks to update raw ingestion stats (default=10)
  "ingestion_raw_update_stats_chunk_frequency": 10,
  // to "specialize" a gulp (from the redis pub/sub consumer point of view) instance, this instance will drain from the redis pub/sub queue just these task types: example values are [ "ingest", "query", "external_query", "rebase" ] or [] / unset to process all task types.
  "instance_roles": [],
  // client certificate password (not supported, ignored)
  "opensearch_client_cert_password": null,
  // how many seconds to wait until timeout (default=60, tune up if you have request timeouts during ingestion/query, 0=no timeout)
  "opensearch_request_timeout": 180,
  // opensearch url (may be overridden by GULP_OPENSEARCH_URL environment variable)
  "opensearch_url": "http://admin:Gulp1234!@localhost:9200",
  // set to false to skip server certificate verification
  "opensearch_verify_certs": false,
  // maximum number of processes to be used for the pool (default=0=number of CPU cores)
  "parallel_processes_max": 0,
  // respawn a process after this number of tasks: set to -1 to never respawn processes, every other value < 100 will be set to 100 (default=100)
  "parallel_processes_respawn_after_tasks": 100,
  // enable cache for plugins (faster loading)
  "plugin_cache_enabled": true,
  // how many documents to return in preview mode (default=100)
  "preview_mode_num_docs": 100,
  // if set, adapts postgresql connection pool size to concurrency (max tasks/num workers)
  "postgres_adaptive_pool_size": true,
  // client certificate password (if any)
  "postgres_client_cert_password": null,
  // set to use SSL to connect to postgresql
  "postgres_ssl": false,
  // postgresql address (may be overridden by GULP_POSTGRES_URL environment variable)
  "postgres_url": "postgresql+psycopg://postgres:Gulp1234!@localhost:5432/gulp",
  // set to false to skip server certificate verification
  "postgres_verify_certs": false,
  // number of times to backoff on opensearch query circuit breaker exception (default=5)
  "query_circuit_breaker_backoff_attempts": 5,
// if set, disables highlights when query circuit breaker is triggered (default=true)
  "query_circuit_breaker_disables_highlights": true,
  // minimum query chunk size limit for circuit breaker backoff (default=100)
  "query_circuit_breaker_min_limit": 100,
  // how many queries to keep in the history per user(default=20, max=50)
  "query_history_max_size": 20,
  // redis payload compression for large pub-sub messages (default=true)
  "redis_compression_enabled": true,
  // compression threshold in kilobytes for redis pub/sub messages: messages bigger than this size will be compressed before publishing (default=256kb)
  "redis_compression_threshold": 256,
  // redis pubsub max chunk size (default=128kb)
  "redis_pubsub_max_chunk_size": 128,
    // redis url (may be overridden by GULP_REDIS_URL environment variable)
  "redis_url": "redis://:Gulp1234!@localhost:6379/0",
  // delete pending request stats on the collab database on gulp shutdown (default=true)
  "stats_delete_pending_on_shutdown": true,
  // how many seconds before automatically delete non-live stats, default=1 day (86400 seconds))
  "stats_ttl": 86400,
  // admin/impersonation token expiration seconds, default=10 min (600 seconds)
  "token_admin_ttl": 600,
  // login token (except admin) expiration time, default=1week (604800 seconds)
  "token_ttl": 604800,
  // if set, websocket delay depends on the number of connected websockets
  "ws_adaptive_rate_limit_delay": true,
  // maximum size of the websocket message queue per client (default=8192 messages). increase if you get "queue full" warnings in the gulp logs.
  "ws_queue_max_size": 8192,
  // timeout in seconds for blocking enqueue operations: if a message cannot be enqueued within this timeout, the client is considered unresponsive and disconnected (default=5.0 seconds)
  "ws_enqueue_timeout": 5.0,
  // queue utilization threshold (0.0-1.0) above which intake throttling begins (default=0.7, i.e. 70% full)
  "ws_throttle_threshold": 0.7,
  // maximum delay in seconds for intake throttling when queue is nearly full (default=0.2 seconds)
  "ws_throttle_max_delay": 0.2,
  // rate limit for websocket (in seconds), introduces a small delay between sends to better handle high throughput when sending data to connected sockets: increment if you get clients disconnections, and also check client code if this happens too often.
  // if `ws_adaptive_rate_limit_delay` is set, this is the base delay used to calculate the real delay depending on the number of connected sockets.
  "ws_rate_limit_delay": 0.01,
  // ai plugin configuration
  "ai_configuration": {
    // The default model to use.
    "default_model": "your_default_model_here",
    // Insert your OpenRouter API key here.
    "openrouter_key": "sk-or-your-openrouter-api-key",
  }
}